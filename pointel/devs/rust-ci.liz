Liz.download("https://raw.githubusercontent.com/emuvi/Lizs/main/pointel/rust-ci.yml", "rust-ci.yml")
File = Liz.read("rust-ci.yml")
Code = Liz.code(File)
Project = Liz.path_name(Liz.wd())
Code:change_all("$name", Project)
Liz.mkdir(".github/workflows")
Code:write(".github/workflows/release.yml")
Liz.rm("rust-ci.yml")